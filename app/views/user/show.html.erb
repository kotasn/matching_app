<h3>こんにちは、<%= current_user.username %>さん</h3>
<%= render 'user/menu' %>

<div class="row">
  <div class="col s5">
    <div class="card horizontal">
      <div class="card-image">
        <%= image_tag 'sample-1.jpg'%>
      </div>
      <div class="card-stacked">
        <div class="card-content">
          <label for="icon_prefix">ニックネーム</label>
          <p><%= current_user.username%></p>
          <label for="icon_prefix">登録アドレス</label>
          <p><%= current_user.email%></p>
          <br>
          <label for="icon_prefix">フォロー数</label>
          <p><%= current_user.followings.count %></p>
          <label for="icon_prefix">フォワー数</label>
          <p><%= current_user.followers.count %></p>
        </div>
      </div>
    </div>
  </div>
            
  <div class="col s3">
    <div class="card-panel green lighten-2">
      <div class="chip">
        <%= current_user.created_at %>
      </div>
      <div class="chip">
        <%= current_user.created_at %>
      </div>
    </div>
  </div>
  <div class="col s3">
    <div class="card-panel green lighten-2">
      <div class="chip">
        <%= current_user.created_at %>
      </div>
      <div class="chip">
        <%= current_user.created_at %>
      </div>
    </div>
  </div>
  <div class="col s6">
    <div class="card-panel blue-grey lighten-5">
      <div class="row">
        <div class="input-field col s12">
          <i class="material-icons prefix">textsms</i>
          <input type="text" id="autocomplete-input" class="autocomplete">
        </div>
      </div>
  </div>
</div>